{
  "name": "server",
  "version": "1.0.0",
  "description": "Quintype validator server implementation",
  "main": "build/main/start.js",
  "module": "build/module/start.js",
  "repository": "https://github.com/Robert-Christopher/server",
  "license": "MIT",
  "keywords": [],
  "scripts": {
    "start": "run-s build:main && node .",
    "start-ts": "TS_NODE_FILES=true ts-node src/start.ts",
    "describe": "npm-scripts-info",
    "build": "run-s clean && run-p build:*",
    "build:main": "tsc -p tsconfig.json",
    "build:module": "tsc -p tsconfig.module.json",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier \"src/**/*.ts\" --write",
    "fix:tslint": "tslint --fix --project .",
    "test": "run-s build test:*",
    "test:unit": "jest",
    "watch": "run-s clean build:main && run-p \"build:main -- -w\" \"test:unit -- --watch\"",
    "nodemon": "nodemon",
    "cov": "run-s build test:unit cov:html && open-cli coverage/index.html",
    "cov:html": "nyc report --reporter=html",
    "cov:send": "nyc report --reporter=lcov && codecov",
    "cov:check": "nyc report && nyc check-coverage --lines 100 --functions 100 --branches 100",
    "doc": "run-s doc:html && open-cli build/docs/index.html",
    "doc:html": "typedoc src/ --exclude **/*.spec.ts --target ES6 --mode file --out build/docs",
    "doc:json": "typedoc src/ --exclude **/*.spec.ts --target ES6 --mode file --json build/docs/typedoc.json",
    "doc:publish": "gh-pages -m \"[ci skip] Updates\" -d build/docs",
    "version": "standard-version",
    "reset": "git clean -dfx && git reset --hard && npm i",
    "clean": "trash build test",
    "prepare-release": "run-s reset test cov:check doc:html version doc:publish",
    "cypress:open": "node_modules/.bin/cypress open",
    "cypress:run": "node_modules/.bin/cypress run"
  },
  "scripts-info": {
    "info": "Display information about the package scripts",
    "build": "Clean and rebuild the project",
    "fix": "Try to automatically fix any linting problems",
    "test": "Lint and unit test the project",
    "watch": "Watch and rebuild the project on save, then rerun relevant tests",
    "cov": "Rebuild, run tests, then create and open the coverage report",
    "doc": "Generate HTML API documentation and open it in a browser",
    "doc:json": "Generate API documentation in typedoc JSON format",
    "version": "Bump package.json version, update CHANGELOG.md, tag release",
    "reset": "Delete all untracked files and reset the repo to the last commit",
    "prepare-release": "One-step: clean, build, test, publish docs, and prep a release"
  },
  "engines": {
    "node": ">=8.9"
  },
  "dependencies": {
    "@quintype/migration-helpers": "^1.14.4",
    "@types/busboy": "^0.2.3",
    "@types/multer": "^1.3.10",
    "@types/split2": "^2.1.6",
    "ajv": "^6.10.2",
    "amphtml-validator": "^1.0.29",
    "array.prototype.flatmap": "^1.2.2",
    "aws-sdk": "^2.632.0",
    "body-parser": "^1.19.0",
    "busboy": "^0.3.1",
    "cheerio": "^1.0.0-rc.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "ejs": "^2.7.1",
    "express": "^4.17.1",
    "lodash": "^4.17.19",
    "node-html-parser": "^1.2.16",
    "nodemailer": "^6.3.1",
    "request": "^2.88.0",
    "request-promise": "^4.2.5",
    "robots-parser": "^2.1.1",
    "sha.js": "^2.4.11",
    "split2": "^3.1.1",
    "ts-json-schema-generator": "^0.58.1",
    "validate.js": "^0.13.1"
  },
  "devDependencies": {
    "@bitjson/npm-scripts-info": "^1.0.0",
    "@bitjson/typedoc": "^0.15.0-0",
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/amphtml-validator": "^1.0.0",
    "@types/array.prototype.flatmap": "^1.2.0",
    "@types/bluebird": "^3.5.28",
    "@types/cheerio": "^0.22.13",
    "@types/compression": "^1.0.1",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/jest": "^24.0.25",
    "@types/js-yaml": "^3.12.1",
    "@types/lodash": "^4.14.144",
    "@types/node": "^13.1.4",
    "@types/nodemailer": "^6.2.2",
    "@types/request-promise": "^4.1.44",
    "ava": "2.2.0",
    "codecov": "^3.5.0",
    "cz-conventional-changelog": "^2.1.0",
    "gh-pages": "^2.0.1",
    "jest": "^24.9.0",
    "nodemon": "^2.0.3",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "open-cli": "^5.0.0",
    "prettier": "^1.18.2",
    "standard-version": "^6.0.1",
    "trash-cli": "^3.0.0",
    "ts-jest": "^24.2.0",
    "ts-node": "^8.10.1",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-immutable": "^6.0.1",
    "typescript": "^3.5.3",
    "cypress": "3.4.1"
  },
  "ava": {
    "failFast": true,
    "files": [
      "build/main/**/*.spec.js"
    ],
    "sources": [
      "build/main/**/*.js"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "prettier": {
    "singleQuote": true
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "exclude": [
      "**/*.spec.js"
    ]
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  }
}
